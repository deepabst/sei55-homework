
<%= form_with model: @singer, local: true do |f| %>

    <%= f.label :name %>
    <br>
    <%= f.text_field :name %>
    <br><br>


    <%= f.label :born %>
    <br>
    <%= f.text_field :born %>
    <br><br>


    <%= f.label :website %>
    <br>
    <%= f.text_area :website %>
    <br><br>

    <%= f.label :image %>
    <br>
    <%= f.text_field :image, placeholder: 'URL here' %>
    <br><br>

    <%= f.submit %>

<% end %>