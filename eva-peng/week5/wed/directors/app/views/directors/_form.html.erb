<%= form_with model: @director, local: true do |f|%>

<%= f.label :name%>
<br>
<%= f.text_field :name%>
<br><br>

<%= f.label :nationality%>
<br>
<%= f.text_field :nationality%>
<br><br>

<%= f.label :dob, 'Date of Birth'%>
<br>
<%= f.date_field :dob%>
<br><br>

<%= f.label :award%>
<br>
<%= f.number_field :award%>
<br><br>


<%= f.label :image%>
<br>
<%= f.text_field :image, placeholder: 'share url here' %>
<br><br>

<%= f.submit %>

<% end %>
